{
  "name": "electron-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "./electron/main.js",
  "build": {
    "productName": "React Electron App",
    "appId": "com.barman.electron",
    "electronVersion": "5.0.1",
    "files": [
      "build/**/*",
      "electron/*"
    ]
  },
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "electron-is-dev": "^1.1.0",
    "materialize-css": "^1.0.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.8.5",
    "sqlite3": "^4.0.8",
    "uuid": "^3.3.2",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^20.40.2",
    "electron-reloader": "^0.3.0",
    "typescript": "^3.4.5",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && npm run electron\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "ebuild": "yarn build && node_modules/.bin/build",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
